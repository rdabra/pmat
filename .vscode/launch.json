   "version": "0.2.0",
   "configurations": [
      {
         "name": "pmat_test",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceFolder}/out/build/covClang/test/pmat_test.exe",
         "args": [],
         "stopAtEntry": false,
         "cwd": "${fileDirname}",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
         "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
         "setupCommands": [
            {
               "description": "Enable pretty-printing for gdb",
               "text": "-enable-pretty-printing",
               "ignoreFailures": true
            },
            {
               "description": "Set Disassembly Flavor to Intel",
               "text": "-gdb-set disassembly-flavor intel",
               "ignoreFailures": true
            }
         ]
      },
      {
         "name": "pmat_main",
