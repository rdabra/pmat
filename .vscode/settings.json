{
   "cSpell.language": "en,pt-BR",
   "cSpell.words": [
      "cholesky",
      "frobenius",
      "Hadamard",
      "pmat"
   ],
   "terminal.integrated.profiles.windows": {
      "MSYS2 UCRT": {
         "path": "cmd.exe",
         "args": [
            "/c",
            "C:\\msys64\\msys2_shell.cmd -defterm -here -no-start -ucrt64"
         ]
      }
   },
   "workbench.colorCustomizations": {
      "[Visual Studio Dark]": {
         "editor.background": "#1D1D21"
      }
   },
   "editor.tokenColorCustomizations": {
      "[Visual Studio Dark]": {
         "textMateRules": [
            {
               "name": "Comment",
               "scope": "punctuation.definition.comment.cpp,comment.line.double-slash.cpp,comment.block.documentation.cpp,punctuation.definition.comment.begin.documentation.cpp,punctuation.definition.comment.end.documentation.cpp",
               "settings": {
                  "fontStyle": "italic",
                  "foreground": "#459872"
               }
            },
            {
               "name": "Source",
               "scope": "source.cpp,code.log",
               "settings": {
                  "foreground": "#B8BABF"
               }
            }
         ]
      }
   },
   "cSpell.enableFiletypes": [
      "cmake"
   ]
}